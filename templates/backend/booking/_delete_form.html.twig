<form method="post" action="{{ path('backend_booking_delete', {'id': booking.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cette réservation ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ booking.id) }}">
    <button class="btn btn-secondary">Supprimer</button>
</form>
